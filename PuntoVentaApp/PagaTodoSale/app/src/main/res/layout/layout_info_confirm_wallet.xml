<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:paddingTop="@dimen/dimen_24"
    android:paddingStart="@dimen/dimen_24"
    android:paddingEnd="@dimen/dimen_24"
    android:paddingBottom="@dimen/dimen_16"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/tvTransactionDate"
        android:text="@string/text_label_transaction_date"
        tools:ignore="MissingConstraints"
        style="@style/Text.Subtitle.Ticket"/>
        <TextView
            android:id="@+id/lblTransactionDate"
            android:layout_marginStart="12dp"
            tools:ignore="MissingConstraints"
            app:layout_constraintStart_toEndOf="@+id/tvTransactionDate"
            style="@style/Text.Content.Ticket"/>

        <TextView
            android:id="@+id/tvTransactionHour"
            android:text="@string/text_label_transaction_hour"
            tools:ignore="MissingConstraints"
            app:layout_constraintTop_toBottomOf="@+id/tvTransactionDate"
            style="@style/Text.Subtitle.Ticket"/>
        <TextView
            android:id="@+id/lblTransactionHour"
            android:layout_marginStart="12dp"
            app:layout_constraintTop_toBottomOf="@+id/tvTransactionDate"
            app:layout_constraintStart_toEndOf="@+id/tvTransactionHour"
            style="@style/Text.Content.Ticket"/>

        <TextView
            android:id="@+id/tvArea"
            android:text="@string/text_label_area"
            tools:ignore="MissingConstraints"
            app:layout_constraintTop_toBottomOf="@+id/tvTransactionHour"
            style="@style/Text.Subtitle.Ticket" />
        <TextView
            android:id="@+id/lblArea"
            android:layout_marginStart="12dp"
            app:layout_constraintTop_toBottomOf="@+id/tvTransactionHour"
            app:layout_constraintStart_toEndOf="@+id/tvArea"
            style="@style/Text.Content.Ticket"/>







        <TextView
            android:id="@+id/tvSellerSale"
            android:text="@string/text_label_seller_sale"
            tools:ignore="MissingConstraints"
            app:layout_constraintTop_toBottomOf="@+id/tvArea"
            style="@style/Text.Subtitle.Ticket" />
        <TextView
            android:id="@+id/lblSellerSale"
            android:layout_marginStart="12dp"
            android:text="@string/text_label_parameter_coin"
            app:layout_constraintTop_toBottomOf="@+id/tvArea"
            app:layout_constraintStart_toEndOf="@+id/tvSellerSale"
            style="@style/Text.Content.Ticket" />



        <TextView
            android:id="@+id/tvSellerCollection"
            android:text="@string/text_label_seller_collection"
            tools:ignore="MissingConstraints"
            app:layout_constraintTop_toBottomOf="@+id/tvSellerSale"
            style="@style/Text.Subtitle.Ticket"/>
        <TextView
            android:id="@+id/lblSellerCollection"
            android:layout_marginStart="12dp"
            android:text="@string/text_label_parameter_coin"
            app:layout_constraintTop_toBottomOf="@+id/tvSellerSale"
            app:layout_constraintStart_toEndOf="@+id/tvSellerCollection"
            style="@style/Text.Content.Ticket" />

</androidx.constraintlayout.widget.ConstraintLayout>