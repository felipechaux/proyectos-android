<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="@dimen/dimen_8"
    android:background="@color/colorGraySilver"
    android:orientation="horizontal">


    <LinearLayout
        android:id="@+id/lyContainerItemGiro"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="@dimen/dimen_16"
        android:layout_weight="1"
        android:orientation="vertical">

        <TextView
            android:id="@+id/txtTitleReference"
            style="@style/Text.Body"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dimen_8"
            android:text="@string/item_giro_consult_reference"
            android:textColor="@color/colorGrayLight" />

        <TextView
            android:id="@+id/txtReference"
            style="@style/Text.Body"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/dimen_8"
            android:text="@string/giro_not_found"
            android:textColor="@color/colorGrayLight" />

        <TextView
            android:id="@+id/txtTitleDocumentNumberOrigin"
            style="@style/Text.Body"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/item_giro_consult_document_origin"
            android:textColor="@color/colorGrayLight" />

        <TextView
            android:id="@+id/txtDocumentNumberOrigin"
            style="@style/Text.Body"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/dimen_8"
            android:text="@string/giro_not_found"
            android:textColor="@color/colorGrayLight" />

        <TextView
            android:id="@+id/txtTitleDocumentNumberDestination"
            style="@style/Text.Body"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/item_giro_consult_document_destination"
            android:textColor="@color/colorGrayLight" />

        <TextView
            android:id="@+id/txtDocumentNumberDestination"
            style="@style/Text.Body"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/dimen_8"
            android:text="@string/giro_not_found"
            android:textColor="@color/colorGrayLight" />


    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="@dimen/dimen_8"
        android:layout_marginEnd="@dimen/dimen_16"
        android:layout_weight="1"
        android:orientation="vertical">

        <TextView
            android:id="@+id/txtTitleValue"
            style="@style/Text.Body"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dimen_8"
            android:text="@string/item_giro_consult_value"
            android:textColor="@color/colorGrayLight" />

        <TextView
            android:id="@+id/txtValue"
            style="@style/Text.Body"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/dimen_8"
            android:text="@string/giro_not_found"
            android:textColor="@color/colorGrayLight" />

        <TextView
            android:id="@+id/txtTitleNombreOrigin"
            style="@style/Text.Body"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/item_giro_consult_name"
            android:textColor="@color/colorGrayLight" />

        <TextView
            android:id="@+id/txtNombreOrigin"
            style="@style/Text.Body"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/dimen_8"
            android:text="@string/giro_not_found"
            android:textColor="@color/colorGrayLight" />

        <TextView
            android:id="@+id/txtTitleNombreDestination"
            style="@style/Text.Body"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/item_giro_consult_name"
            android:textColor="@color/colorGrayLight" />

        <TextView
            android:id="@+id/txtNombreDestination"
            style="@style/Text.Body"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/dimen_8"
            android:text="@string/giro_not_found"
            android:textColor="@color/colorGrayLight" />

        <!-- Item compact sale point  -->

        <LinearLayout
            android:id="@+id/lyReference"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:visibility="gone" />

        <LinearLayout
            android:id="@+id/lyDocumentNumberOrigin"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:visibility="gone" />


        <LinearLayout
            android:id="@+id/lyDocumentNumberDestination"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:visibility="gone" />


        <LinearLayout
            android:id="@+id/lyValue"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:visibility="gone" />


        <LinearLayout
            android:id="@+id/lyNombreOrigin"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:visibility="gone" />


        <LinearLayout
            android:id="@+id/lyNombreDestination"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:visibility="gone" />


        <LinearLayout
            android:id="@+id/lyAgencia"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:visibility="gone" />

        <TextView
            android:id="@+id/txtAgency"
            style="@style/Text.Body"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/dimen_8"
            android:text="@string/giro_not_found"
            android:textColor="@color/colorGrayLight"
            android:visibility="gone" />


    </LinearLayout>


</LinearLayout>