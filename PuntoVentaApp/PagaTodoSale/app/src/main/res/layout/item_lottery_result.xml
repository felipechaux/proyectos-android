        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dimen_4"
        android:layout_marginBottom="@dimen/dimen_4">

    <TextView
            android:id="@+id/itemLotteryResultNumber"
            android:layout_width="match_parent"
