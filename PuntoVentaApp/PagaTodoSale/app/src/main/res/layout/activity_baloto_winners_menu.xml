<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".views.homemenu.HomeMenuActivity">

    <include
        layout="@layout/main_toolbar"
        android:id="@+id/mainToolbar"/>
    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rvMenus"
        android:background="@color/colorWhite"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginStart="@dimen/dimen_margin_menus"
        android:layout_marginEnd="@dimen/dimen_margin_menus"
        android:layout_marginBottom="20dp"
        tools:listitem="@layout/item_main_menu">
        </androidx.recyclerview.widget.RecyclerView>
</LinearLayout>